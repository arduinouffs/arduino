  pinMode(10, OUTPUT);
  pinMode(11, OUTPUT);
  pinMode(12, OUTPUT);
  pinMode(13, OUTPUT);
}

void loop()
{
  digitalWrite(0, HIGH);
  delay(delay_);
  digitalWrite(0, LOW);
  delay(delay_);
  digitalWrite(1, HIGH);
  delay(delay_);
  digitalWrite(1, LOW);
  delay(delay_);
  digitalWrite(2, HIGH);
  delay(delay_);
  digitalWrite(2, LOW);
  delay(delay_);
  digitalWrite(3, HIGH);
  delay(delay_);
  digitalWrite(3, LOW);
  delay(delay_);
  digitalWrite(4, HIGH);
  delay(delay_);
  digitalWrite(4, LOW);
  delay(delay_);
  digitalWrite(5, HIGH);
  delay(delay_);
  digitalWrite(5, LOW);
  delay(delay_);
  digitalWrite(6, HIGH);
  delay(delay_);
  digitalWrite(6, LOW);
  delay(delay_);
  digitalWrite(7, HIGH);
  delay(delay_);
  digitalWrite(7, LOW);
  delay(delay_);
  digitalWrite(8, HIGH);
  delay(delay_);
  digitalWrite(8, LOW);
  delay(delay_);
  digitalWrite(9, HIGH);
  delay(delay_);
  digitalWrite(9, LOW);
  delay(delay_);
  digitalWrite(10, HIGH);
  delay(delay_);
  digitalWrite(10, LOW);
  delay(delay_);
  digitalWrite(11, HIGH);
  delay(delay_);
  digitalWrite(11, LOW);
  delay(delay_);
  digitalWrite(12, HIGH);
  delay(delay_);
  digitalWrite(12, LOW);
  delay(delay_);
  digitalWrite(13, HIGH);
  delay(delay_);
  digitalWrite(13, LOW);
  delay(delay_);
}